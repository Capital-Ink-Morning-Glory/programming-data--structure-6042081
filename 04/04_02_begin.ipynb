    "# 创建水果集合\n",
    "common_fruits = set([\"苹果\", \"香蕉\", \"葡萄\"])\n",
    "\n",
    "fruit = \"西瓜\"\n",
    "\n",
    "# 检查元素是否在集合中\n",
    "if fruit in common_fruits: \n",
    "    print(fruit + \"在集合中\")\n",
    "else:\n",
    "    print(fruit + \"不在集合中\")"
   ]
